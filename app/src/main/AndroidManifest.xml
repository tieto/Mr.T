<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tieto.systemmanagement" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <uses-permission android:name="android.permission.UPDATE_APP_OPS_STATS" />

    <application
        android:name=".SysManageApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SystemManagementActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BasicActivity" >
        </activity>
        <activity
            android:name=".NotificationActivity"
            android:label="@string/title_activity_notification" >
        </activity>
        <activity
            android:name=".processmanage.ProcessActivity"
            android:label="@string/title_activity_process_management" >
        </activity>
        <activity
            android:name=".MemoryActivity"
            android:label="@string/title_activity_memory" >
        </activity>
        <activity
            android:name=".AppActivity"
            android:label="@string/title_activity_app" >
        </activity>
        <activity
            android:name=".authority.AuthorityActivity"
            android:label="@string/title_activity_authority" >
        </activity>
        <activity
            android:name=".SelfStartActivity"
            android:label="@string/title_activity_self_start" >
        </activity>
        <activity
            android:name=".trafficmonitor.TrafficActivity"
            android:label="@string/title_activity_traffic"
            android:icon="@drawable/t_common_title_icon">
        </activity>
        <activity
            android:name=".diskmonitor.DiskActivity"
            android:label="@string/title_activity_disk" >
        </activity>
        <activity
            android:name=".intercept.views.InterceptActivity"
            android:label="@string/title_activity_no_spam" >
        </activity>
        <activity
            android:name=".NetControlActivity"
            android:label="@string/title_activity_net_control" >
        </activity>
        <activity
            android:name=".BatteryActivity"
            android:label="@string/title_activity_battery" >
        </activity>
        <activity android:name=".trafficmonitor.views.FireWallManageActivity"
            android:label="联网管理"
            android:icon="@drawable/t_common_title_icon">
        </activity>
        <activity android:name=".trafficmonitor.views.NetSpeedActivity"
            android:label="网络测速"
            android:icon="@drawable/t_common_title_icon"></activity>

    </application>

</manifest>
